{
  "buildCommand": null,
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/about", "destination": "/index.html" },
    { "source": "/news", "destination": "/index.html" },
    { "source": "/contact", "destination": "/index.html" },
    { "source": "/products", "destination": "/index.html" },
    { "source": "/products/gut-health", "destination": "/index.html" },
    { "source": "/products/feed-quality", "destination": "/index.html" },
    { "source": "/products/mycotoxin-eliminator", "destination": "/index.html" },
    { "source": "/products/nutrition", "destination": "/index.html" },
    { "source": "/certifications", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
